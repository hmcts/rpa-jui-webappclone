{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run build-universal",
    "build-universal": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "ng build --prod && ng run jui:server",
    "start": "node server.js",
    "start-dev": "ng serve",
    "start-dev-proxy": "ng serve --proxy-config proxy.config.json",
    "start-dev-node": "export JUI_ENV=local; node dev-server.js",
    "start-dev-docker-proxy": "export JUI_ENV=docker; ng serve --proxy-config proxy.config.json",
    "start-dev-docker-node": "export JUI_ENV=docker; node dev-server.js",
    "dev": "yarn build-universal && concurrently \"yarn dev-frontend\" \"yarn dev-backend\"",
    "dev-backend": "nodemon server.js --watch api --watch src --watch middleware --watch lib --watch proxies --watch config --delay 3000ms -e js,ts,html,scss",
    "start-json-server": "export JUI_ENV=mock; node ./mock-server/mockserver.js",
    "dev-frontend": "ng-udkc",
    "lint": "yarn lint:ng && yarn lint:node || true",
    "lint:fix": "(yarn lint:ng:fix || true) && (yarn lint:node:fix || true)",
    "lint:ng": "ng lint jui",
    "lint:ng:fix": "ng lint jui --fix",
    "lint:node": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o ./reports/lint/node/index.html",
    "lint:node:fix": "eslint . --fix",
    "test": "npm run test:node && npm run test:ng",
    "test:node": "jasmine --config=spec/support/jasmine.json",
    "test:ng": "ng test",
    "test:nsp": "nsp check",
    "test:a11y": "echo 'ally testing not implemented'",
    "test:routes": "echo 'routes testing not implemented'",
    "test:coverage": "npm run test:coverage:node && npm run test:coverage:ng",
    "test:coverage:ng": "ng test --code-coverage",
    "test:coverage:node": "yarn istanbul cover -x \"**//**/**.spec.js\" -x \"**//config/**\" -x \"**//spec/**\" jasmine --config=spec/support/jasmine.json",
    "test-all": "echo 'test all not implemented'",
    "test:nsp-warn": "echo \"Test nsp warn\"",
    "test:snyk": "snyk test",
    "test:smoke": "echo 'smoke test not implemented'",
    "test:functional": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js",
    "test:functional:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js --local",
    "health-check": "echo 'healthcheck not implemented'",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:codacy-upload": "cat ./reports/tests/coverage/lcov.info | codacy-coverage",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/lcov.info",
    "snyk-protect": "snyk protect",
    "e2e": "protractor ./test/e2e/config/conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.1.0",
    "@angular/cli": "^6.1.0",
